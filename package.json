{
  "name": "crete-your-life-api",
  "version": "0.0.1",
  "description": "GraphQL API for Crete your Life trip booking application",
  "author": "Coral Software Mateusz Tokarski",
  "license": "GPL-3.0",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Mutisz/crete-your-life-api"
  },
  "scripts": {
    "dev": "npm-run-all --parallel start playground",
    "start": "nodemon -e ts,graphql -x ts-node -r dotenv/config src/index.ts",
    "playground": "graphql playground",
    "build": "rm -rf dist && rm -rf src/generated && npm-run-all prisma-generate resolver-generate && tsc",
    "resolver-generate": "graphqlgen",
    "prisma-generate": "prisma generate",
    "prisma-deploy": "prisma deploy"
  },
  "dependencies": {
    "@types/shortid": "0.0.29",
    "aws-sdk": "^2.399.0",
    "fixer-node": "^2.0.0",
    "graphql": "^14.1.1",
    "graphql-tag": "^2.10.1",
    "graphql-tools": "^4.0.4",
    "graphql-yoga": "^1.17.4",
    "http-errors": "^1.7.1",
    "prisma-binding": "^2.3.2",
    "prisma-client-lib": "^1.26.3",
    "shortid": "^2.2.14"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.120",
    "dotenv": "^6.2.0",
    "graphql-resolver-codegen": "0.0.3",
    "graphqlgen": "0.6.0-rc5",
    "nodemon": "^1.18.9",
    "npm-run-all": "^4.1.5",
    "prettier": "1.16.4",
    "prisma": "^1.26.3",
    "ts-node": "^8.0.2",
    "tslint": "^5.12.1",
    "typescript": "^3.3.3",
    "typescript-tslint-plugin": "0.3.1"
  }
}
